          <stringProp name="WebDriverSampler.script">import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.ui.ExpectedConditions;
import org.openqa.selenium.support.ui.WebDriverWait;
import java.time.Duration;

WDS.sampleResult.sampleStart();

WDS.browser.get(&quot;${jiraUrl}&quot;);
WebElement emailBox = WDS.browser.findElement(By.cssSelector(&quot;#username&quot;));
emailBox.sendKeys(new String[] { &quot;${email}&quot; });
WDS.browser.findElement(By.cssSelector(&quot;#login-submit&quot;)).click();
WebElement passwordBox = WDS.browser.findElement(By.cssSelector(&quot;#password&quot;));
passwordBox.sendKeys(new String[] { &quot;${password}&quot; });
WDS.browser.findElement(By.cssSelector(&quot;#login-submit&quot;)).click();
WebDriverWait wait = new WebDriverWait(WDS.browser, Duration.ofSeconds(20));
wait.until(ExpectedConditions.visibilityOfElementLocated(By.cssSelector(&quot;#createGlobalItem&quot;)));

WDS.browser.get(&quot;${jiraUrl}${getIssuePath}&quot;);
WebElement iframeElem = WDS.browser.findElement(By.cssSelector(&quot;iframe[data-testid=\&quot;hosted-resources-iframe\&quot;]&quot;));
//Switch to iframe and interaction
WDS.browser.switchTo().frame(iframeElem);
           
WebDriverWait wait2 = new WebDriverWait(WDS.browser, Duration.ofSeconds(20));
wait2.until(ExpectedConditions.visibilityOfElementLocated(By.cssSelector(&quot;.panel-page .cell-key&quot;)));

WDS.sampleResult.sampleEnd();

</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
